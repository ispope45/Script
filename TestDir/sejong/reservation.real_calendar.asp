							<!-- [s]골프예약 조회/취소 -->


<script language="javascript">
$(function(){

	$('.resvEntrust')
	.css({'cursor':'pointer'})
 	.click(function(){

 		var _date = $(this).attr('date');
		var _course = $(this).attr('course');
		var _coursedesc = $(this).attr('coursedesc');
		var _time = $(this).attr('time');
		var _rsvnno = $(this).attr('resvno');
		var _caddy = $(this).attr('caddy');

		if(_caddy == 'N'){
			alert('해당 시간은 노캐디 타임입니다. 비회원에게 위임이 불가능합니다.');
		}else{

			$('#txtDate').val(_date)
			$('#txtCourse').val(_course);
			$('#txtCourseDesc').val(_coursedesc);
			$('#txtTime').val(_time);
			$('#txtRsvnNo').val(_rsvnno)

			if(!confirm('해당 예약건을 위임하시겠습니까?')) return false;

			$('#frm1').attr('action', 'ref.asp').submit();
		}

 	})

 	$('.resvEntrust1')
	.css({'cursor':'pointer'})
 	.click(function(){

 		var _date = $(this).attr('date');
		var _course = $(this).attr('course');
		var _coursedesc = $(this).attr('coursedesc');
		var _time = $(this).attr('time');
		var _rsvnno = $(this).attr('resvno');

		$('#txtDate').val(_date)
		$('#txtCourse').val(_course);
		$('#txtCourseDesc').val(_coursedesc);
		$('#txtTime').val(_time);
		$('#txtRsvnNo').val(_rsvnno)

		if(!confirm('해당 예약건을 위임하시겠습니까?')) return false;

		$('#frm1').attr('action', 'ref2.asp').submit();

 	})

	$('.resvModify')
	.css({'cursor':'pointer'})

	.click(function(){
		var _url = '';

		if($(this).attr('refuse') < 3){
			alert('3일 이전 예약 건입니다. 예약 변경이 불가합니다.');
			return;
		}

		switch($(this).attr('penalty')){
			case 'Y':
				_url = 'real_timeList.asp';
				break;
			case 'N':
				_url = 'real_calendar.asp';
				break;
		}

		$('#txtDate').val($(this).attr('date'))
		$('#txtRsvnNo').val($(this).attr('resvno'))
		$('#txtMode').val('U')

		$('#frm1').attr('action', _url).submit();
	})

	$('.resvCancle')
	.css({'cursor':'pointer'})
	.click(function(){

		var _date = $(this).attr('date');
		var _course = $(this).attr('course');
		var _time = $(this).attr('time');
		var _rsvnno = $(this).attr('resvno');

		$('#txtFlag').val('checkPenalty');
		$('#txtDate').val(_date)
		$('#txtCourse').val(_course);
		$('#txtTime').val(_time.replace(':',''));
		$('#txtRsvnNo').val(_rsvnno)

		fnc_ajax(
			$('#frm1').serialize(),
			'json',
			'reservation_proc.asp',
			function(rtnResult, rtnMsg, rtnData){
				if(rtnResult == 'Y'){		//일반취소
					if(!confirm(_date+'일 '+_time+' 타임을 취소하시겠습니까?')) return false;

					$('#txtFlag').val('rsvnCancle');

					fnc_ajax(
						$('#frm1').serialize(),
						'json',
						'reservation_proc.asp',
						function(rtnResult, rtnMsg, rtnData){
							if(rtnResult == 'Y'){
								alert('예약이 정상적으로 취소되었습니다.')
								$('#frm1').attr('action', 'real_calendar.asp').submit();
							}
							else{
								alert(rtnMsg);
							}
						},
						function(rtnMsg){
						}
					);

				}
				else{ 	//위약취소
					if(!confirm(rtnMsg+'\n'+_date+'일 '+_time+' 타임을 취소하시겠습니까?')) return false;

					$('#txtFlag').val('rsvnCancle');

					fnc_ajax(
						$('#frm1').serialize(),
						'json',
						'reservation_proc.asp',
						function(rtnResult, rtnMsg, rtnData){
							if(rtnResult == 'Y'){
								alert('예약이 정상적으로 취소되었습니다.')
								$('#frm1').attr('action', 'real_calendar.asp').submit();
							}
							else{
								alert(rtnMsg);
							}
						},
						function(rtnMsg){
						}
					);

				}
			},
			function(rtnMsg){
			}
		);

	})

})

function chkValue(form){
    var st_date, ed_date
    st_date = form.st_date.value
    ed_date = form.ed_date.value

    if(st_date>ed_date){
        alert("검색시작일이 종료일보다 빠릅니다.\n\n다시한번 확인하여 주십시요.");
        form.st_date.focus();
    }else{
        form.submit();
    }
}

