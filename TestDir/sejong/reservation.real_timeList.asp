<SCRIPT LANGUAGE="JavaScript">
$(function(){

	$('.resv')
	.css({'cursor':'pointer'})
	.click(function(){
		var _url;

		if(fnc_validate($('#frm1 *[validate]')) == false) return false;

		var _date = $(this).attr('date');
		var _course = $(this).attr('course');
		var _time = $(this).attr('time');
		var _mode = $(this).attr('mode');
		var _stat = $(this).attr('stat');
		var _caddy = $(this).attr('caddy');
		var _tel = $('#tel1').val()+'-'+$('#tel2').val()+'-'+$('#tel3').val();
		var _modeDesc = '';
		var _courseDesc = '';
		var _caddyDesc = '';
		var _caddyDesc2 = '';
		var _caddyDesc3 = '';

		$('#txtFlag').val('setGolfTime');
		$('#txtDate').val(_date);
		$('#txtCourse').val(_course);
		$('#txtTime').val(_time.replace(':',''));
		$('#txtMode').val(_mode);
		$('#txtStat').val(_stat);
		$('#txtCaddy').val(_caddy);

		if(_caddy=='N'){
			$('#txtRemark').val('노캐디');
			_caddyDesc = '(노캐디)';
			_caddyDesc2 = '선택하신 타임은 "노캐디"시간 입니다. 회원 동반 필수입니다.\n';
			_caddyDesc3 += '\n<노캐디 안전수칙>\n';
			_caddyDesc3 += '* 노캐디 운영 수칙을 꼭 숙지하시길 바랍니다.\n';
			_caddyDesc3 += '① 지정된 운전자 한분만 운전 가능하며, 면허증은 필수 지참입니다.\n';
			_caddyDesc3 += '② 노캐디 안전 수칙에 대해 설명을 전달 받았으며, 운전자외 운전시 안전사고 책임은 지정된 운전자에게 있습니다.\n';
			_caddyDesc3 += '③ 플레이중 안전사고는 동반자 전원의 책임이므로 안전사고에 유의하셔야 합니다.\n';
			_caddyDesc3 += '④ 당사는 플레이에 도움을 드릴수 있으나, 안전사고에 대해 어떠한 책임을 지지 않습니다.\n';
			_caddyDesc3 += '⑤ 남성 70세이상, 여성 65세이상 이신분은 안전상 카트운전이 불가합니다.\n';
			_caddyDesc3 += '위 사항을 모두 준수하시길 당부드립니다.\n';
			_caddyDesc3 += '-세종에머슨컨트리클럽-';

		}else{
			$('#txtRemark').val('');
		}

		if(_mode=='I'){
			_modeDesc = '예약';
		}else if(_mode=='U'){
			_modeDesc = '변경';
		}

		switch(_course){
			case '1':
				_courseDesc = '레이크';
				break;
			case '2':
				_courseDesc = '밸리';
				break;
			case '3':
				_courseDesc = '마운틴';
				break;
		}

		var _msg = _modeDesc+' 되었습니다.'+_caddyDesc+'\n예약일시 : 2022년 5월 18일 '+_time+'\n예약코스 : '+_courseDesc+' 코스\n긴급연락처 : '+_tel;

		if(!confirm(''+_caddyDesc2+_modeDesc+'하시겠습니까?'+_caddyDesc3)) return false;

		fnc_ajax(
			$('#frm1').serialize(),
			'json',
			'reservation_proc.asp',
			function(rtnResult, rtnMsg, rtnData){
				if(rtnResult == 'Y'){
					alert(_msg);
					$('#txtRsvnNo').val('');
					$('#txtMode').val('');
					$('#frm1').attr('action', 'real_calendar.asp').submit();
				}
				else{
					alert(rtnMsg);
				}
			},
			function(rtnMsg){
			}
		);

	})

	$('#Refresh').click(function(){
		$('#frm1').attr('action', 'real_timeList.asp');
		Refresh(frm1);
	})
})
</SCRIPT>





