function chkValue(form){
    var st_date, ed_date
    st_date = form.st_date.value
    ed_date = form.ed_date.value

    if(st_date>ed_date){
        alert("검색시작일이 종료일보다 빠릅니다.\n\n다시한번 확인하여 주십시요.");
        form.st_date.focus();
    }else{
        form.submit();
    }
}

